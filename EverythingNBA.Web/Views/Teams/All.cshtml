@model ICollection<string>

<div class="p-3 m-0">
    <h2 class="p-2">All Teams</h2>

    @foreach (var team in Model)
    {
        <h3>
            <a class="ml-3 mb-1 container-fluid w-100 player-name" asp-controller="Team" asp-action="TeamDetails" asp-route-teamName="@team">@team</a>
        </h3>
    }
</div>
